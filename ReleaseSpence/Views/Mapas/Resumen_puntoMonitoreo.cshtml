@model ReleaseSpence.Models.Sensores

<b>Punto de Monitoreo: </b> <b>@ViewBag.punto.nombre</b> <br />

@if (ViewBag.sensoresList != null)
{

    <b>Sensores Asociados: </b> <br />
    foreach (var item in ViewBag.sensoresList)
    {
        @item.nombre <br />
    }
    @Html.ActionLink("Ver Graficos", "Index", "Multipiezometro", new { id = ViewBag.punto.idPuntoMonitoreo, idTipo = 7, precision = true, datos = 200, desde = DateTime.Now.AddMonths(-3).ToString("dd-MM-yyyy HH:mm:ss").Replace(":", "").Replace("-", "").Replace(" ", ""), hasta = DateTime.Now.ToString("dd-MM-yyyy HH:mm:ss").Replace(":", "").Replace("-", "").Replace(" ", "") }, new { @class = "btn btn-primary", target = "_blank" })}

else
{
    <p>Sin sensores asociados.</p>
}

